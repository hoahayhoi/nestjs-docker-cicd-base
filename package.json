  "scripts": {
    "build": "nest build",
    "prepare": "husky",
    "lint": "eslint \"{src,apps,test}/**/*.ts\" --fix",
    "lint:fix": "eslint . --config eslint.config.mjs --fix",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "format:check": "prettier --check \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "dotenv -e .env.development -- nest start --watch",
    "start:test": "dotenv -e .env.testing -- nest start --watch",
    "dev": "dotenv -e .env.development -- nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "dotenv -e .env.production -- node dist/src/main",
    "migrate:dev": "dotenv -e .env.development -- npx prisma migrate dev",
    "migrate:test": "dotenv -e .env.testing -- npx prisma migrate dev",
    "prisma-gen-client": "npx prisma generate",
    "migrate:dev-gen": "dotenv -e .env.development -- npx prisma migrate dev --name",
    "migrate:prod": "dotenv -e .env.production -- npx prisma migrate deploy",
    "seed": "dotenv -e .env.development -- npx ts-node prisma/seed.ts",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "deploy:dev": "npm install && npx dotenv -e .env.development -- npm run build && pm2 restart ecosystem.config.js",
    "pm2:dev": "dotenv -e .env.development -- node dist/src/main"
  },
