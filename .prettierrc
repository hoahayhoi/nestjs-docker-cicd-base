{
  "printWidth": 120,
  "tabWidth": 2,
  "useTabs": false,
  "semi": true,
  "singleQuote": true,
  "quoteProps": "consistent",
  "trailingComma": "all",
  "bracketSpacing": true,
  "bracketSameLine": false,
  "arrowParens": "always",
  "endOfLine": "crlf",
  "proseWrap": "always",
  "overrides": [
    {
      "files": ["*.md", "*.json"],
      "options": {
        "printWidth": 100
      }
    }
  ]
}
